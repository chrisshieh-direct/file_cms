<html>
<head>
  <link rel="stylesheet" href="/stylesheets/application.css">
</head>
<body>
    <% if session[:error] %>
      <div class="alert"><h2 class="alert"><%= session.delete(:error) %></h2></div>
    <% end %>
    <% if session[:success] %>
      <div class="alert"><h2 class="alert"><%= session.delete(:success) %></h2></div>
    <% end %>
  <%= yield %>
</body>
</html>
